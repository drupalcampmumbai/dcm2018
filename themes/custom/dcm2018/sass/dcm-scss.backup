}

/*end maintainace page*/

/*Footer*/
footer.footer {
    padding: 0;
    border: 0;
    margin-top: 0;
    width: 100%;
    max-width: inherit;

    #block-drupalapprenctices {
        padding: 20px 0;
        background-color: #bce8f1;

        .drupal-apprentice {
            width: $page-width;
            margin: 0 auto;

            .image-wrapper {
                text-align: right;
            }
        }
    }

    #block-joinda {
        padding: 20px 0;
        background-color: #F6F6F2;

        .join-drup-assoc {
            width: $page-width;
            margin: 0 auto;

            .image-wrapper {
                text-align: right;
            }
        }
    }

    #dcm2017-footer {
        position: relative;

        .google-maps {
            height: 200px;

            @media screen and (max-width: 960px) {
                height: 245px;
            }
        }

        .footer-copy {
            position: absolute;
            right: 0;
            top: 0;
            z-index: 1;
            background-color: rgba(42, 169, 222, 0.69);
            padding: 40px 20px 40px 50px;
            height: 100%;
            font-size: 16px;
            line-height: 22px;
            width: 50%;
            color: #fff;

            @media screen and (max-width: 960px) {
                position: absolute;
                right: inherit;
                top: inherit;
                padding: 15px;
                height: 50%;
                font-size: 14px;
                line-height: 16px;
                width: 100%;
                bottom: 0;
            }

            a {
                color: #fff;
                text-decoration: underline;
                transition: all 0.3s linear 0s;

                &:hover {
                    color: $base-color;
                }
            }
        }
    }

    .dcm2017-footer {
        background-color: #fff;

        .dcm2017-footer-bottom {
            padding: 15px 0 0;
            text-align: center;

            @media screen and (max-width: 960px) {
                padding: 10px 0 0;
            }

            a {
                color: #343434;
                display: inline-block;
                letter-spacing: 8px;
                text-transform: uppercase;

                @media screen and (max-width: 960px) {
                    letter-spacing: 4px;
                }
            }
        }

        .made-with {
            text-align: center;
            color: #343434;
            font-size: 11px;
            padding-bottom: 15px;
            line-height: 12px;

            @media screen and (max-width: 960px) {
                padding-bottom: 10px;
            }
        }

        .powered-by {
            position: absolute;
            right: 10px;
            bottom: 5px;

            @media screen and (max-width: 960px) {
                position: static;
                text-align: center;
            }

            img {
                @media screen and (max-width: 960px) {
                    width: 120px;
                }
            }
        }
    }
}

/*End Footer*/