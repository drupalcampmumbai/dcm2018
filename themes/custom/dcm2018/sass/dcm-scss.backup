@import "dcm2017-variables";

/*Generic css*/
/*Body & Other general elements*/
body {
    border: 0 none;
    color: $base-color;
    font-family: $proxima-regular;
    font-size: $basefont-size;
    line-height: $baseline-height;
    margin: 0;
    padding: 0;
    transition: all 0.35s linear 0s;

    @media screen and (max-width: 320px) {
        min-width: 320px;
    }

    div * {
        outline: none;
    }
}

h1, h2, h3, h4, h5, h6 {
    font-weight: 500;
    margin: 0 0 15px;
}

h1, .h1 {
    font-size: 30px;
    line-height: 30px;

    @media screen and (max-width: 960px) {
        font-size: 22px;
        line-height: 24px;
    }
}

h2, .h2 {
    font-size: 25px;
    line-height: 25px;

    @media screen and (max-width: 960px) {
        font-size: 20px;
        line-height: 22px;
    }
}

h3, .h3 {
    font-size: 20px;
    line-height: 20px;

    @media screen and (max-width: 960px) {
        font-size: 18px;
        line-height: 20px;
    }
}

h4, .h4 {
    font-size: 18px;
    line-height: 20px;

    @media screen and (max-width: 960px) {
        font-size: 16px;
        line-height: 18px;
    }
}

h5, .h5 {
    font-size: 16px;
    line-height: 18px;

    @media screen and (max-width: 960px) {
        font-size: 14px;
        line-height: 16px;
    }
}

h4, .h4 {
    font-size: 14px;
    line-height: 16px;
}

p {
    margin: 0 0 10px;
}

a:hover, a:focus {
    background: transparent;
    color: $hover-color;
    text-decoration: none;
    outline: 0;
}

img {
    @media screen and (max-width: 960px) {
        max-width: 100%;
    }
}

a {
    color: $link-color;
    text-decoration: none;
    outline: 0;
    transition: all 0.5s linear 0s;

    img {
        border: 0;
        outline: 0;
    }
}

div.warning, table tr.warning {
    white-space: normal;
}

.element-invisible {
    display: none;
}

ul, .block ul, ol {
    margin: 0.5em 0 1em;
    padding: 0 0 0 1.0em;

    li, li.leaf {
        display: list-item;
        list-style-image: none;
        list-style-type: disc;
        margin: 0;
        padding: 0 0 5px;
    }
}

ul.list-inline {
    margin: 0.5em 0 1em;
    padding: 0 0 0 1.0em;

    li {
        display: inline-block;
        padding-left: 5px;
        padding-right: 5px;
        margin: 0;
        list-style-image: none;
    }
}

.pagination > li {
    display: inline;
}

.nav > li > a:hover, .nav > li > a:focus {
    background-color: transparent;
}

ul.contextual-links, .block ul.contextual-links, ol.contextual-links {
    margin: 0;
    padding: 0;

    li, li.leaf {
        list-style-image: none;
        list-style-type: none;
        margin: 0;
        padding: 0;
    }
}

ul.tabs.primary {
    white-space: normal;
}

.tabs--primary.nav.nav-tabs {
    margin: 0 0 15px;

    li {
        padding: 0;

        a {
            padding: 5px 10px;
        }
    }
}

table {
    border: 0;

    tbody {
        border: 0;

        tr {
            th {
                border: 0;
                border-right: 1px solid #ddd;
                background-color: #eee;
                color: #000;
                font-weight: 600;
                font-size: 15px;
                padding: 8px;

                &:last-child {
                    border-right: 0;
                }
            }

            td {
                border: 1px solid #ddd;
                color: #000;
                font-weight: normal;
                padding: 8px;

                &:first-child {
                    border-left: 0;
                }

                &:last-child {
                    border-right: 0;
                }
            }

            &:last-child {
                td {
                    border-bottom: 0;
                }
            }
        }
    }
}

.label {
    color: #000;
    padding: 0;
    text-align: left;
}

/*End Body & Other general elements*/

/*Header*/
header.container, .navbar.container {
    width: 100%;
    max-width: inherit;
    border: 0;
    border-radius: 0;
    z-index: 2;
}

.path-frontpage {
    .page-header {
        display: none;
    }

    header#navbar {
        background-image: url(../images/screen-4.jpg);
        background-position: 0 0;
        background-size: cover;
        background-repeat: no-repeat;
        background-attachment: fixed;
        height: 85vh;
        padding: 15px 0 0;

        @media screen and (max-width: 960px) {
            padding: 15px;
            height: 380px;
        }

        .navbar-header {
            float: none;
            max-width: inherit;
            margin: 0;

            .logo {
                float: none !important;
                display: table;
                text-align: center;
                margin: auto;
                clear: both;

                img {
                    width: 200px;

                    @media screen and (max-width: 960px) {
                        width: 150px;
                    }
                }
            }

            #dcm-sticky-menu {
                clear: both;
                position: static;

                nav {
                    border-bottom: 1px dashed #fff;
                    border-top: 1px dashed #fff;
                    margin: 25px 0;

                    ul {
                        text-align: center;

                        li {
                            a {
                                padding: 10px 15px;
                            }

                            .menu.dropdown-menu {
                                background-color: rgba(0, 0, 0, 0.56);
                            }
                        }
                    }
                }
            }
        }
    }

    .main-container {
        background-image: none;
        background-color: rgba(241, 241, 241, 0.5);
        padding-bottom: 0;

        @media screen and (max-width: 960px) {
            padding-bottom: 30px;
        }

        .col-sm-12[role="heading"] {
            box-shadow: inset 0px 11px 8px -5px #cccccc,inset 0px -11px 8px -5px #dddddd;
            padding: 30px 0 20px;
        }

        .region-header {
            max-width: $page-width;
            margin: 0 auto;

            @media screen and (max-width: 960px) {
                max-width: inherit;
            }
        }

        .page-container {
            .page-container-inner {
                padding-right: 0;
                padding-left: 0;
                background-color: transparent;

                @media screen and (max-width: 960px) {
                    padding-right: 15px;
                    padding-left: 15px;
                }

                aside, .sidebar {
                    h2.block-title {
                        background: transparent none repeat scroll 0 0;
                        box-shadow: none;
                        font-size: 17px;
                    }

                    .view-content, .field--name-body {
                        padding: 0;
                        background-color: transparent;
                        box-shadow: none;
                    }
                }
            }
        }
    }
}

.path-contact {
    .page-container {
        .page-container-inner {
            .view-content {
                padding: 0;
                background-color: transparent;
                box-shadow: none;
            }
        }
    }
}

header#navbar {
    background-image: url(../images/sea-link.jpg);
    background-position: 0 -415px;
    background-size: cover;
    background-repeat: no-repeat;
    background-attachment: fixed;
    height: 120px;
    margin-bottom: 0;

    @media screen and (max-width: 960px) {
        height: 110px;
        background-position: -345px -327px;
    }

    .navbar-toggle {
        @media screen and (max-width: 960px) {
            position: absolute;
            top: 0;
            right: 15px;
        }
    }

    .navbar-header {
        float: none;
        max-width: $page-width;
        margin: 0 auto;

        .logo {
            float: left !important;
            display: block;
            z-index: 1;
            position: relative;

            img {
                width: 140px;

                @media screen and (max-width: 960px) {
                    width: 95px;
                }
            }
        }

        #dcm-sticky-menu {
            clear: none;
            width: 100%;
            position: absolute;
            z-index: 0;
            left: 0;
            bottom: 0;

            @media screen and (max-width: 960px) {
                display: none;
            }

            nav {
                background-color: rgba(0, 0, 0, 0.4);

                .menu {
                    margin: 0 auto;
                    transition: all 0.5s linear 0s;
                    border-bottom: 0;
                    border-top: 0;
                    background-color: transparent;
                    width: $page-width;
                    text-align: right;
                }
            }
        }
    }

    .navbar-collapse {
        @media screen and (max-width: 960px) {
            position: fixed;
            width: 250px;
            top: 0;
            padding: 0;
            height: 100% !important;
            background-color: #fff;
            overflow-y: auto;
            z-index: 100;
            left: -500px;
            margin: 0;
            border-bottom: 4px solid $link-color !important;
            box-shadow: 0 2px 5px #666 !important;
            transition: all 0.35s linear 0s;
        }

        &.in {
            @media screen and (max-width: 960px) {
                left: 0px;
                transition: all 0.35s linear 0s;
            }
        }

        #dcm-sticky-menu {
            display: none !important;

            @media screen and (max-width: 960px) {
                display: block !important;
            }
        }
    }

    .slogan {
        background: rgba(0, 0, 0, 0) url("../images/bg_slogan.png") repeat scroll left top;
        border-radius: 10px;
        color: #fff;
        margin: 5px auto 0;
        padding: 30px;
        max-width: $page-width;
        text-align: center;

        p {
            a {
                color: #ffffff;
            }
        }

        @media screen and (max-width: 960px) {
            padding: 10px;
        }

        h2 {
            font-size: 40px;
            font-weight: 900;
            margin-bottom: 25px;
            line-height: 40px;

            @media screen and (max-width: 960px) {
                font-size: 25px;
                margin-bottom: 15px;
                line-height: 30px;
            }
        }

        p {
            font-size: 30px;
            font-weight: 100;

            @media screen and (max-width: 960px) {
                font-size: 20px;
            }
        }
    }
}

/*End Header*/

/*Main menu*/
header#navbar {
    .navbar-toggle {
        margin-right: 0;
        margin-top: 39px;
        border: 0;
        padding-right: 0;

        &:hover, &:focus {
            background-color: transparent;
        }

        .icon-bar {
            background-color: #fff;
            width: 30px;
            height: 3px;
        }
    }

    .navbar-collapse {
        border: 0;
        box-shadow: none;
    }

    #dcm-sticky-menu {
        clear: both;

        .menu.nav {
            margin: 30px 0;
            padding: 0;
            transition: all 0.5s linear 0s;
            text-align: center;
            border-bottom: 1px dashed #fff;
            border-top: 1px dashed #fff;
            background-color: rgba(0, 0, 0, 0.4);

            @media screen and (max-width: 960px) {
                margin: 0;
                border: 0;
                background-color: #fff;
            }

            h2.welcomeUserMenu {
                background-color: $link-color;
                background-image: none;
                box-shadow: 0 2px 5px #666;
                padding: 10px;
                font-size: 18px;
                margin: 5px 0 10px;
                text-transform: capitalize;
                position: relative;
                line-height: 20px;
                text-align: left;

                @media (min-width: 960px) and (max-width: 2000px) {
                    display: none;
                }

                span {
                    font-weight: 600;
                    color: #fff;
                }

                .uName {
                    display: inline-block;
                    font-weight: normal;
                    white-space: nowrap;
                    width: 8em;
                    overflow: hidden;
                    text-overflow: ellipsis;
                    position: relative;
                    top: 5px;
                    margin: 0 0 0 5px;
                }

                .menuSlideClose {
                    color: #fff;
                    cursor: pointer;
                    font-size: 18px;
                    font-weight: normal;
                    height: 25px;
                    line-height: 25px;
                    margin: 0;
                    position: absolute;
                    text-align: center;
                    top: 10px;
                    right: 8px;
                    width: 25px;
                    z-index: 1001;
                }
            }

            li {
                margin: 0;
                padding: 0;
                display: inline-block;

                @media screen and (max-width: 960px) {
                    display: block;
                }

                &:last-child {
                    a {
                        border-bottom: 0;
                    }
                }

                &.open {
                    a {
                        color: #fef5cc;
                        background-color: transparent;
                    }
                }

                a {
                    text-transform: uppercase;
                    color: #fff;
                    transition: all 0.3s linear 0s;
                    font-family: $proxima-semibold;
                    font-size: 16px;
                    position: relative;
                    display: block;
                    padding: 10px 11px;

                    @media screen and (max-width: 960px) {
                        font-family: $proxima-regular;
                        font-size: 14px;
                        color: $base-color;
                        padding: 10px;
                        text-align: left;
                        border-bottom: 1px solid #ddd;
                    }

                    &:hover, &.is-active {
                        color: #fef5cc;
                        border-color: #fef5cc;
                        transition: all 0.3s linear 0s;

                        @media screen and (max-width: 960px) {
                            color: $link-color;
                            border-color: #ddd;
                        }

                        &:before {
                            right: 0;
                        }
                    }

                    &:before {
                        content: "";
                        position: absolute;
                        z-index: 0;
                        left: 0;
                        right: 100%;
                        bottom: 0;
                        background: #fef5cc;
                        height: 2px;
                        -webkit-transition-property: right;
                        transition-property: right;
                        -webkit-transition-duration: 0.4s;
                        transition-duration: 0.4s;
                        -webkit-transition-timing-function: linear;
                        transition-timing-function: linear;

                        @media screen and (max-width: 960px) {
                            height: 1px;
                            background: $link-color;
                        }
                    }

                    .caret {
                        @media screen and (max-width: 960px) {
                            float: right;
                            margin-top: 6px;
                        }
                    }

                    .caret-up {
                        width: 0;
                        height: 0;
                        border-left: 4px solid rgba(0, 0, 0, 0);
                        border-right: 4px solid rgba(0, 0, 0, 0);
                        border-bottom: 4px solid;
                        display: inline-block;
                        margin-left: 2px;
                        vertical-align: middle;

                        @media screen and (max-width: 960px) {
                            float: right;
                            margin-top: 6px;
                        }
                    }
                }

                .menu.dropdown-menu {
                    background-color: rgba(129, 115, 131, 0.79);
                    width: auto;
                    border-radius: 0;
                    text-align: left;

                    @media screen and (max-width: 960px) {
                        background-color: rgb(255, 255, 255);
                        float: none;
                        width: 100%;
                        margin: 0;
                        position: static;
                        box-shadow: none;
                        border: 0;
                    }

                    li {
                        display: block;

                        &:last-child {
                            a {
                                border-bottom: 0;
                            }
                        }

                        a {
                            border-bottom: 1px dashed #eee;
                            padding: 10px 15px;
                            color: #fff;

                            @media screen and (max-width: 960px) {
                                color: $base-color;
                                padding: 10px 20px;
                            }

                            &:hover, &.is-active {
                                color: #fef5cc;
                                background-color: transparent;

                                @media screen and (max-width: 960px) {
                                    color: $link-color;
                                }

                                &:before {
                                    display: none;
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}

.sliderO {
    overflow: hidden;
    position: fixed;
    height: 100%;
    width: 100%;
    margin-left: 250px;
    transition: all 0.35s linear 0s;
}

.menuSlideOverlay {
    background-color: rgba(0, 0, 0, 0.6);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    right: 0;
    z-index: 10;
    bottom: 0;
    transition: all 1s linear 0s;
}

/*End Main menu*/

/*Main content*/
.main-container {
    border: 0 none;
    margin: 0 auto;
    min-height: 550px;
    width: 100%;
    max-width: inherit;
    padding-left: 0;
    padding-right: 0;
    padding-bottom: 85px;
    clear: both;
    background-image: url(../images/footer-buildings.png);
    background-repeat: repeat-x;
    background-position: 0 bottom;

    .close {
        line-height: 0.5;
        top: 5px;
        right: 0;
    }

    .page-container {
        .page-container-inner {
            background-color: rgba(255, 255, 255, 0.8);
            width: $page-width;
            margin: 0 auto;
            padding: 30px 15px;
            overflow: hidden;

            @media screen and (max-width: 960px) {
                width: 100%;
                padding: 15px;
            }

            #middle-content, aside, .col-sm-3, .col-sm-9 {
                @media screen and (max-width: 960px) {
                    padding-left: 0;
                    padding-right: 0;
                }
            }

            .block {
                margin: 0 0 20px;
            }
        }
    }
}

.slideanim {
    visibility: hidden;
    visibility: visible\9;

    /*For old IE browsers IE6-8 */
    &.slide {
        visibility: visible;
        animation: slide 1s;
    }

    &:after {
        /* useful when its child elements are float:left; */
        content: "";
        display: table;
        clear: both;
    }
}

@keyframes slide {
    0% {
        opacity: 0;
        transform: translateY(50%);
    }

    100% {
        opacity: 1;
        transform: translateY(0);
    }
}

/*End Main content*/

/*Content bottom*/
.content_bottom {
    clear: both;

    .block-views-block-sponsors-silver-sponsor-slideshow, .block-views-blocksponsors-partners-slideshow {
        @media screen and (max-width: 960px) {
            width: 100%;
        }

        .block-title {
            font-size: 17px;
            font-weight: 600;
        }

        .view-content {
            .views-row {
                width: 25%;

                @media screen and (max-width: 960px) {
                    width: 40%;
                    margin: 0 15px 30px 0 !important;
                }

                @media screen and (max-width: 480px) {
                    width: 90%;
                    margin-right: 0 !important;
                }
            }
        }
    }

    /**  Style for social block. **/
    #block-facebookfeed, #block-twitterfeeds {
        width: 380px;
        float: left;
        height: 600px;
        border: 1px solid #fff;
        padding: 20px;
        margin: 0 4%;
        background-color: #fff;
        -moz-box-shadow: 0 10px 20px rgba(0, 0, 0, 0.19), 0 6px 6px rgba(0, 0, 0, 0.23);
        -webkit-box-shadow: 0 10px 20px rgba(0, 0, 0, 0.19), 0 6px 6px rgba(0, 0, 0, 0.23);
        box-shadow: 0 10px 20px rgba(0, 0, 0, 0.19), 0 6px 6px rgba(0, 0, 0, 0.23);
        border-radius: 5px;
        -moz-transition: all 0.25s;
        -o-transition: all 0.25s;
        -webkit-transition: all 0.25s;
        transition: all 0.25s;
        overflow: hidden;
        margin-bottom: 40px;
        text-align: center;

        @media screen and (max-width: 960px) {
            width: 48%;
            margin: 0 1% !important;
            height: 350px;
        }

        @media screen and (max-width: 480px) {
            width: 100%;
            margin: 0 !important;
            height: 300px;
            float: none;
            clear: both;
        }
    }

    section#block-views-block-sponsors-gold-sponsor-slideshow {
        clear: both;
    }
}

/*End Content bottom*/

/*sidebars*/
aside, .sidebar {
    margin: 0;
    padding: 0;

    @media screen and (max-width: 960px) {
        float: none;
        margin: 20px 0 0;
    }

    h2.block-title {
        background: $link-color none repeat scroll 0 0;
        font-size: 17px;
        color: #fff;
        font-family: $proxima-semibold;
        line-height: 18px;
        margin: 0;
        padding: 10px 15px;
        text-transform: capitalize;
        box-shadow: 3px 3px 1px rgb(137, 153, 156);
    }

    .block {
        margin: 0 0 20px;

        .view-content, .field--name-body {
            padding: 10px 15px;
            background-color: rgba(221, 221, 221, 0.41);
            box-shadow: 3px 3px 1px rgb(168, 179, 181);
        }
    }
}

.sidebar-first {
    aside, .sidebar {
        width: 30%;

        @media screen and (max-width: 960px) {
            width: 100%;
        }
    }

    #content, .col-sm-9 {
        width: 70%;

        @media screen and (max-width: 960px) {
            width: 100%;
        }
    }
}

.sidebar-second {
    aside, .sidebar {
        width: 30%;

        @media screen and (max-width: 960px) {
            width: 100%;
        }
    }

    #content, .col-sm-9 {
        width: 70%;

        @media screen and (max-width: 960px) {
            width: 100%;
        }
    }
}

.two-sidebars {
    aside, .sidebar {
        width: 20%;

        @media screen and (max-width: 960px) {
            width: 100%;
        }
    }

    #content, .col-sm-6 {
        width: 60%;

        @media screen and (max-width: 960px) {
            width: 100%;
        }
    }
}

.no-sidebars {
    section {
        width: 100%;
    }

    #content, .col-sm-12 {
        padding: 0;
    }
}

.well {
    background-color: transparent;
    border: 0 none;
    border-radius: 0;
    box-shadow: none;
    margin-bottom: 0;
    padding: 0;
}

/*Social media*/
#block-socialblock {
    position: fixed;
    right: 0;
    top: 30%;
    z-index: 999;
    width: 5%;

    @media screen and (max-width: 960px) {
        display: none;
    }

    .social-media-list {
        > span {
            text-align: right;
            color: #fff;
            display: block;
            margin: 0 0 1px;

            .stButton {
                margin: 0;

                .stButton_gradient {
                    border: 0;
                    height: auto;
                    padding: 0;
                    border-radius: 0;
                }

                .stMainServices, .chicklets {
                    display: inline-block;
                    font: normal normal normal 19px/1 FontAwesome;
                    text-rendering: auto;
                    -webkit-font-smoothing: antialiased;
                    -moz-osx-font-smoothing: grayscale;

                    /*Added important: to override default css which is added by module using js*/
                    background-image: none !important;
                    width: 40px;
                    height: auto;
                    float: none;
                    text-indent: 0px;
                    color: #fff;
                    font-size: 0;
                    background-color: #000;
                    padding: 8px;
                    transition: all 0.5s linear 0s;
                    text-align: center;

                    @media screen and (max-width: 960px) {
                        width: 30px;
                    }

                    &:hover {
                        width: 50px;
                        background-color: $link-color;
                        text-align: left;
                        opacity: 1;

                        @media screen and (max-width: 960px) {
                            width: 40px;
                        }
                    }

                    &.st-facebook-counter {
                        &:before {
                            font-size: 19px;
                            content: "\f09a";

                            @media screen and (max-width: 960px) {
