/*Banner slideshow*/
#block-dcm2017-views-block-banner-block-1 {
    .view-banner {
        overflow: hidden;

        .views_slideshow_controls_text {
            position: absolute;
            top: 44%;
            width: 100%;
            z-index: 10;
            box-sizing: border-box;

            .views_slideshow_controls_text_previous {
                a {
                    display: inline-block;
                    position: absolute;
                    left: 0;
                    z-index: 100;
                    text-indent: -9999px;
                    transition: all 0.2s linear 0s;

                    &:hover {
                        left: -5px;

                        &:before {
                            border-color: #000;
                        }
                    }

                    &:before {
                        border-bottom: 2px solid #fff;
                        border-left: 2px solid #fff;
                        content: "";
                        display: inline-block;
                        height: 1.7em;
                        left: 20px;
                        margin: 3px 0 0 5px;
                        position: absolute;
                        top: 8px;
                        transform: rotate(45deg);
                        -moz-transform: rotate(45deg);
                        -webkit-transform: rotate(45deg);
                        -o-transform: rotate(45deg);
                        -ms-transform: rotate(45deg);
                        transition: all 0.2s ease-in-out 0s;
                        width: 1.7em;
                    }
                }
            }

            .views_slideshow_controls_text_pause {
                display: none;
            }

            .views_slideshow_controls_text_next {
                a {
                    display: inline-block;
                    right: 0;
                    position: absolute;
                    z-index: 100;
                    text-indent: -9999px;
                    transition: all 0.2s linear 0s;

                    &:hover {
                        right: -5px;

                        &:before {
                            border-color: #000;
                        }
                    }

                    &:before {
                        border-bottom: 2px solid #fff;
                        border-right: 2px solid #fff;
                        content: "";
                        display: inline-block;
                        height: 1.7em;
                        right: 20px;
                        margin: 3px 0 0 5px;
                        position: absolute;
                        top: 8px;
                        transform: rotate(-45deg);
                        -moz-transform: rotate(-45deg);
                        -webkit-transform: rotate(-45deg);
                        -o-transform: rotate(-45deg);
                        -ms-transform: rotate(-45deg);
                        transition: all 0.2s ease-in-out 0s;
                        width: 1.7em;
                    }
                }
            }
        }
    }
}

