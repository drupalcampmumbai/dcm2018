                                font: normal normal normal 20px/1 FontAwesome;
                                text-rendering: auto;
                                -webkit-font-smoothing: antialiased;
                            }

                            .views-field-title:before {
                                content: "\f022";
                            }

                            .views-field-uid:before {
                                content: "\f0a1";
                            }

                            .views-field-field-session-track:before {
                                content: "\f073";
                            }
                        }
                    }

                    .view-display-id-session_listing_block .more-link {
                        padding: 10px 16px;
                        background-color: #f9f9f9;
                    }
                }
            }
        }
    }
}

/*End Form*/

/*Buttons & message*/
.user-logged-in {
    .register-now {
        display: none;
    }
}

.btn, .btn-default {
    background: none repeat scroll 0 0 $link-color;
    border: 0 none;
    border-radius: 3px;
    box-shadow: none;
    color: #fff;
    padding: 10px 15px;
    margin: 10px 10px 10px 0;
    transition: all 0.5s linear 0s;

    &:hover, &:focus, &:active, &.active, .open .dropdown-toggle.btn-default {
        background: none repeat scroll 0 0 $hover-color;
        color: #fff;
    }
}

.btn-default:focus, .btn-default.focus {
    transition: all 0.5s linear 0s;
    background-color: #666;
}

a.btn:focus, a.btn:hover {
    transition: all 0.5s linear 0s;
    background-color: $hover-color;
}

div.error, table tr.error {
    background-color: transparent;
    color: #a94442;
}

.alert {
    padding: 8px;
}

/*End Buttons & message*/

/*Modal popup*/
#modalContent {
    margin-left: -500px / 2;
    left: 50%;

    .ctools-modal-dialog {
        width: 500px;
        border-radius: 0;
        margin: 0;
        height: 400px;

        .modal-content {
            background-color: #fff;
            border: 0 none;
            border-radius: 0;
            box-shadow: none;
            margin-top: 0;
            padding: 0;
            position: relative;

            .modal-header {
                background-image: none;
                background-color: #ccc;
                border: 0 none;
                height: auto;
                margin: 0;
                padding: 10px 20px;
                text-transform: uppercase;

                .close {
                    color: #fff;
                    opacity: 1;
                }

                #modal-title {
                    color: #fff;
                    font-size: 20px;
                    line-height: 22px;
                    text-align: left;
                }
            }

            .modal-body {
                color: $base-color;
                height: auto;
                max-height: inherit;
                overflow-y: auto;
                padding: 10px 20px;
                position: static;
                width: 100%;
            }
        }
    }
}

#modalBackdrop {
    background-color: #666;
}

.modal-open {
    overflow-y: auto;
    overflow-x: hidden;
}

#freeze-height {
    height: 100%;
}

/*End Modal popup*/

/*Comment form*/
.comment {
    background-color: transparent;
    border: 0 none;
    border-bottom: 1px solid #ccc;
    margin: 0 0 20px;
    padding: 0 10px 20px;
    word-wrap: break-word;
    background-color: transparent;
    border: 0 none;
    margin: 0 0 15px;
    padding: 0 10px 15px;
    word-wrap: break-word;

    .user-picture {
        float: left;
        margin: 0 15px 5px 0;
    }
}

#comments {
    margin: 20px 0 0;

    .comment {
        &:last-child {}

        .links.inline {
            display: none;
        }

        background-color: #f9f9f9;
        border-left: 1px solid #ccc;
        padding-left: 15px;
        padding-top: 10px;
    }

    .field-name-field-review-title {
        font-size: 18px;
        font-weight: 600;
        color: $base-color;

        .field-item {
            &:before {
                content: open-quote;
            }

            &:after {
                content: close-quote;
            }
        }
    }

    h3 {
        margin: 0 0 3px;

        .permalink {
            color: $base-color;
            font-size: 18px;
            font-weight: 500;

            &:before {
                content: open-quote;
            }

            &:after {
                content: close-quote;
            }
        }
    }

    .submitted {
        color: $base-color;
        font-style: italic;
        font-weight: 400;
        margin-bottom: 5px;
        font-size: 11px;

        &::before {
            content: "(";
        }

        &::after {
            content: ")";
        }
    }

    ul.links {
        margin: 5px 0 0;
        padding: 0;
        text-align: right;

        li {
            margin: 0;
            padding: 0 5px;
            text-transform: capitalize;
            display: inline-block;
        }
    }
}

.field.field-name-field-your-message.field-type-text-long.field-label-hidden {
    font-size: 13px;
    margin: 15px 0 0;
}

[id^='comment-form'] {
    [id^='edit-author'] {
        .username {
            color: $base-color;
            font-size: 13px;
        }
    }

    .form-item {
        label {
            color: $base-color;
            float: left;
            font-weight: 500;
            width: 15%;
        }

        .form-text {
            height: auto;
            padding: 7px 10px;
            width: 635px;
        }

        .form-textarea-wrapper {
            width: 83%;

            textarea {
                font-weight: normal;
                min-height: 150px;
                padding: 10px;
                resize: vertical;
                width: 100%;
            }
        }
    }

    .field-name-field-review-title {
        .form-item {
            label {
                margin: 10px 0 0;
            }
        }
    }

    .form-actions.form-wrapper {
        margin-left: 143px;
    }

    .form-submit {
        margin: 0 10px 0 0;
        padding: 5px 20px;
        text-transform: uppercase;

        &:hover {
            background-color: #cf2122;
        }
    }

    .filter-wrapper {
        display: none;
    }

    [id^='edit-preview'] {
        display: none;
    }
}

.resizable-textarea {
    .grippie {
        display: none;
    }
}

#review {
    h2.title {
        font-size: 25px;
        font-weight: 600;
        line-height: 28px;
        margin: 0 0 10px;
    }
}

.comment-edit {
    border-left: 1px solid #005a8c;
    border-right: 1px solid #005a8c;
}

.field.field-name-field-rating.field-type-fivestar.field-label-above {
    clear: both;
    display: block;
    margin: 10px 0 0;
}

.indented {
    margin-left: 25px;
}

.reviews-back-to-page {
    margin: 20px 0 0;

    a {
        background-color: #e42627;
        border-radius: 5px;
        color: $base-color;
        display: inline-block;
        padding: 6px 12px;
        text-decoration: none;
        text-transform: none;
    }
}

.reviews.no-reviews {
    a {
        color: $base-color;
        text-decoration: underline;
        text-transform: capitalize;
    }
}

/*End Comment form*/

/*maintainace page*/
body.maintenance-page {
    background-position: 0 0;
    margin: 0 auto;
    width: 650px;
    text-align: center;
    padding: 20px;
    line-height: 22px;
    font-size: 16px;

    header {
        padding: 0;

        h1 {
            display: none;
        }
    }

